Algoritmo "semnome"

Var
   grau_fahrenheit: real
   grau_celcius: real
Inicio
    escreval("Conversão de Fahrenheit para Celcius.")
   escreval("Digite em Fahrenheit:")
   leia(grau_fahrenheit)

   grau_celcius <- 5 * (grau_fahrenheit - 32)/9

   escreval(grau_fahrenheit,"ºF equivalem a",grau_celcius,"ºC")


Fimalgoritmo