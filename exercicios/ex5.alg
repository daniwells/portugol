algoritmo "Exercício 3"

var
   lado_quadrado: real
   area_quadrado: real

inicio
   escreval("----------------")
   escreval("AREA DO QUADRADO")
   escreval("----------------")
   escreval("Digite o lado do quadrado:")
   leia(lado_quadrado)

   area_quadrado <- lado_quadrado ^ 2

   escreval("A área desse quadrado é:",area_quadrado)

fimalgoritmo